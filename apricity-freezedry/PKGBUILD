pkgname=apricity-freezedry
pkgver=0.1.7
pkgrel=1
pkgdesc='Apricity configuration tool'
arch=('any')
url='https://github.com/Apricity-OS/apricity-keyring'
license=('GPL')
install="${pkgname}.install"
source=('git+https://github.com/Apricity-OS/apricity-freezedry#branch=dev')
sha256sums=('SKIP')

package() {
	#cd "${srcdir}/${pkgname}-${pkgver}"
	cd "${srcdir}/apricity-keyring"
	cp freezedry.sh /usr/bin/freezedry
}
